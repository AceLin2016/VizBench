  "showfps": 1,
  "trail_enable": 0,
  "trail_amount": 0.90,
  "camera": -1,    # a value of -1 disables the camera input
# "camera": 1,  # the index of the camera, given to cvCreateCameraCapture
  "foo": 0
}
