{

# If pipeset is "" or unspecified, the pipset is obtained from state.json,
# so that it's always whatever pipeset you were last using.
# If you always want to start on a given pipeset, specify it here
# or on the command-line.
  "pipeset" : "",

  "ffglpath": "ffglplugins;ffglplugins_resolume;ffglplugins_loopycam",
  "ff10path": "ff10plugins;ff10plugins_pete;ff10plugins_loopycam",

  # "window_x": 10,
  # "window_y": 10,
  # "window_width": 1920,
  # "window_height": 1080,
  # "window_width": 640,
  # "window_height": 480,

  ### Settings from VizCamp
  # "window_x": -1920,
  # "window_y": 0,
  # "window_width": 1920,
  # "window_height": 1080,

  "window_monitor": 0,
  "window_x": 100,
  "window_y": 100,
  "window_width": 1600,
  "window_height": 900,

  "preview_monitor": 0,
  "preview_x": 1800,
  "preview_y": 200,
  "preview_width": 800,
  "preview_height": 450,

  "font": "poorich.ttf",
  "fontsize": 48,

  "spout": 0,
  "showfps": 0,
  "autoload": 1,
  "autosave": 1,
  "fps": 60,

# The camera value is the index given to cvCreateCameraCapture.
# A value of -1 disables the camera input.
  "camera": -1,

  "foo": 0  # silly, make sure the last entry doesn't have a comma
}
